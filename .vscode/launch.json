{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug simple-tt: Next.js App",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "skipFiles": ["<node_internals>/**"]
      },
      {
        "name": "Attach to Next.js",
        "type": "node",
        "request": "attach",
        "port": 9229,
        "restart": true,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }